{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
		"baseUrl": ".",
    "outDir": "./dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
		"paths": {
			"finpoq/*": ["./src/*"],
			"finpoq-core/*": ["../core/src/*"]
		},
  },
  "references": [
		{
			"path": "../core"
		}
	]
}
